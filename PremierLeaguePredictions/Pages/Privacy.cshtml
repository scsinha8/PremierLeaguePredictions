@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<table>
    <tr>
        <th>Team Name</th>
    </tr>
    @foreach(var item in Model.PLTable)
    {
        <tr>
            <td>
                @Html.DisplayTextFor((modelItem=>item.clubName))
            </td>
        </tr>
    }
</table>
